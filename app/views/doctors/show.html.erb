<h1><% @doctor.name %></h1>
<%=@doctor.department%>
<%@doctor.appointments.each do |a|%>
<!-- human readable time -->
  <p><%=a.appointment_datetime.strftime("%B %d, %Y at %H:%M")%></p>
  <p><%=link_to a.patient.name, patient_path(a.patient)%></p>
  <hr>
<%end%>

